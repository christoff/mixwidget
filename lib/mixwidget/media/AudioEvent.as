ISH:String = "scrub finish";        //not used right now    public static const ENABLE_PLAY:String = "enable play";    public static const ENABLE_PREV:String = "enable prev";    public static const ENABLE_NEXT:String = "enable next";    public static const DISABLE_PLAY:String = "disable play";    public static const DISABLE_PREV:String = "diable prev";    public static const DISABLE_NEXT:String = "disable next";        public var track_num:Number;    public var track:XML;    public var status:AudioStatus;        public function AudioEvent(type:String, track:XML=null, status:AudioStatus = null, bubbles:Boolean=false, cancelable:Boolean=false)    {      this.track = track;      if(status != null) this.status = status;      super(type, bubbles, cancelable);    }      }}